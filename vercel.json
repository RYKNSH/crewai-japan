{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist/public",
    "installCommand": "npm install -g pnpm && pnpm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}